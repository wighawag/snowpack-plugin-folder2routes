{{#each routes}}
{{#unless this.async}}
import {{this.componentName}} from '{{this.importPath}}';
{{/unless}}
{{/each}}

export default [
{{#each routes}}
  {
    name: '{{this.name}}',
    path: '{{this.path}}',
    {{#if this.async}}
    asyncComponent: () => import('{{this.importPath}}'),
    {{else}}
    component: {{this.componentName}},
    {{/if}}
  },
{{/each}}
];
